<h1>Hey there, <%= current_user.name %>!</h1>

<p>Welcome Back! Looking for some inspiration? Check out some art supplies here!</p>

<% @categories.each do |category| %>
    <p><%= category.name %></p>
    <p><%= category.art_style %></p>
    <% @categories.supplies.each do |supply| %>
        <p><%= supplies.medium %></p>
        <p><%= supplies.tools %></p>
    <% end %>
<% end %>

<form action="/logout" method="POST">
    <input type="submit" value="Logout">
</form>