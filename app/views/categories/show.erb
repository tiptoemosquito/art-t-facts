<h3><%= @category.art_form %></h3>
<hr>
<br>
    <% @supplies.each do |s|%>
        <p> Medium: <%= s.medium %></p>
        <p> Tools: <%= s.tools %></p><br>

        <form action="/supplies/<%= s.id %>" method="post">
            <input type="hidden" id="hidden" name="_method" value="DELETE">
            <input type="submit" value="Delete">
        </form><br>
        <a href="/supplies/<%= s.id %>/edit"><button>Edit</button></a>
        <br>
    <% end %>

<br>

<br>
<a href="/categories/<%= @category.id %>/supplies/new"><button>Add New Supplies</button></a><br>
<br>
<a href="/categories"><button>Go Back</button></a><br>